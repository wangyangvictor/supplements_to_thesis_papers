\newcommand\smallpsum{\mathop{{\sum\nolimits^{\mathrlap{\prime}}}}}
%https://tex.stackexchange.com/questions/455827/formatting-of-a-star-in-the-right-upper-corner-of-a-product-symbol
\newcommand{\psum}{\sideset{}{^\prime}\sum}
%https://www.tutorialspoint.com/tex_commands/sideset.htm
%https://tex.stackexchange.com/questions/29690/how-to-type-primed-sum
\newcommand\smallpprod{\mathop{{\prod\nolimits^{\mathrlap{\prime}}}}}
\newcommand{\pprod}{\sideset{}{^\prime}\prod}
\newcommand\smallpmax{\mathop{{\max\nolimits^{\mathrlap{\prime}}}}}
\newcommand{\pmax}{\sideset{}{^\prime}\max}
\newcommand\smallpmin{\mathop{{\min\nolimits^{\mathrlap{\prime}}}}}
\newcommand{\pmin}{\sideset{}{^\prime}\min}
\newcommand\smallpsup{\mathop{{\sup\nolimits^{\mathrlap{\prime}}}}}
\newcommand{\psup}{\sideset{}{^\prime}\sup}
\newcommand\smallpinf{\mathop{{\inf\nolimits^{\mathrlap{\prime}}}}}
\newcommand{\pinf}{\sideset{}{^\prime}\inf}

%2018/06/12 v4: fixed amsthm environments for question, problem, exercise
%http://mirror.las.iastate.edu/tex-archive/macros/latex/required/amscls/doc/amsthdoc.pdf
%Plain: Theorem, Lemma, Corollary, Proposition, Conjecture, Criterion, Assertion
\theoremstyle{plain}% default

\newtheorem{sidethm}{Side Theorem}[section]
\newtheorem{sidelem}[sidethm]{Side Lemma}
\newtheorem{sideprop}[sidethm]{Side Proposition}
\newtheorem{sidecor}[sidethm]{Side Corollary}
\newtheorem{sideconj}[sidethm]{Side Conjecture}

\newcommand{\pf}[1]{\begin{proof}#1 \end{proof}}

\newtheorem{theorem}{Theorem}[section] %start new section counter
\newcommand{\thm}[1]{\begin{theorem}#1 \end{theorem}}

\newtheorem{theorem-definition}[theorem]{Theorem-Definition} %share with theorem counter
\newcommand{\thmdefn}[1]{\begin{theorem-definition}#1 \end{theorem-definition}}

\newtheorem{lemma}[theorem]{Lemma} %share with theorem counter
\newcommand{\lem}[1]{\begin{lemma}#1 \end{lemma}}

\newtheorem{lemma-definition}[theorem]{Lemma-Definition} %share with theorem counter
\newcommand{\lemdefn}[1]{\begin{lemma-definition}#1 \end{lemma-definition}}

\newtheorem{proposition}[theorem]{Proposition} %share with theorem counter
\newcommand{\prop}[1]{\begin{proposition}#1 \end{proposition}}

\newtheorem{proposition-definition}[theorem]{Proposition-Definition} %share with theorem counter
\newcommand{\propdefn}[1]{\begin{proposition-definition}#1 \end{proposition-definition}}

\newtheorem{corollary}[theorem]{Corollary} %share with theorem counter
\newcommand{\cor}[1]{\begin{corollary}#1 \end{corollary}}

\newtheorem{corollary-definition}[theorem]{Corollary-Definition} %share with theorem counter
\newcommand{\cordefn}[1]{\begin{corollary-definition}#1 \end{corollary-definition}}

\newtheorem{fact}[theorem]{Fact}
%like assertion?

\newtheorem{conjecture}[theorem]{Conjecture} %share with theorem counter
\newcommand{\cnj}[1]{\begin{conjecture}#1 \end{conjecture}}




\theoremstyle{definition}
%Definition, Condition, Problem, Example, Exercise, Algorithm, Question, Axiom, Property, Assumption, Hypothesis

\newtheorem{sidedef}[sidethm]{Side Definition}
\newtheorem{sideex}[sidethm]{Side Example}
\newtheorem{sideques}[sidethm]{Side Question}

\newtheorem{definition}[theorem]{Definition} %share with theorem counter
\newcommand{\defn}[1]{\begin{definition}#1 \end{definition}}

\newtheorem{example}[theorem]{Example} %share with theorem counter
\newcommand{\ex}[1]{\begin{example}#1 \end{example}}

\newtheorem{exercise}[theorem]{Exercise} %share with theorem counter
\newcommand{\exer}[1]{\begin{exercise}#1 \end{exercise}}

\newtheorem{problem}[theorem]{Problem} %share with theorem counter
\newcommand{\prob}[1]{\begin{problem}#1 \end{problem}}

\newtheorem{question}[theorem]{Question} %share with theorem counter
\newcommand{\ques}[1]{\begin{question}#1 \end{question}}


\theoremstyle{remark}
%Remark, Note, Notation, Claim, Summary, Acknowledgment, Case, Conclusion

\newtheorem{sidermk}[sidethm]{Side Remark}

\newtheorem{remark}[theorem]{Remark} %share with theorem counter
\newcommand{\rmk}[1]{\begin{remark}#1 \end{remark}}

\newtheorem{note}[theorem]{Note} %share with theorem counter
%\newtheorem{comment}[theorem]{Comment} %share with theorem counter. Used by versions package somehow?
\newtheorem{case}[theorem]{Case} %same as theorem counter, avoid using if possible.
\newtheorem*{solution}{Solution} %similar to proof, but without automatic box...

\newtheorem*{claim}{Claim} %share with theorem counter

\newtheorem{observation}[theorem]{Observation} %share with theorem counter
\newcommand{\obs}[1]{\begin{observation}#1 \end{observation}}




